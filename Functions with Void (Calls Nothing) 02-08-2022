void eep(char color, int num) {
   
   
    Beep(num, num);
   
   
    if (color == 'r')
        cout << "red" << endl;
    else if (color == 'b')
        cout << "blue" << endl;
    else if (color == 'g')
        cout << "green" << endl;
    else if (color == 'y')
        cout << "yellow" << endl;
   

}
